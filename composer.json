{
    "name": "kep/cs-fixer",
    "description": "Customized php cs fixer",
    "keywords": [
        "microframework",
        "helper"
    ],
    "homepage": "https://github.com/KefirchickTod/customized-cs-fixer",
    "license": "MIT",
    "authors": [
        {
            "name": "Zakhar",
            "email": "zacharpu2@gmail.com",
            "homepage": "https://github.com/KefirchickTod"
        }
    ],
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.65",
        "phpunit/phpunit": "^11.5"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "KepCustomFixer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "kep-php-cs-fixer": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "test": "vendor/bin/phpunit --colors=always --verbose"
    }
}

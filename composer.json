{
    "name": "kep/cs-fixer",
    "description": "Customized php cs fixer",
    "keywords": [
        "microframework",
        "helper"
    ],
    "homepage": "http://github.com/slimphp/Slim-Skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Zakhar",
            "email": "zacharpu2@gmail.com",
            "homepage": "http://www.joshlockhart.com/"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.65"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "KepCustomFixer\\": "src/"
        }
    },
    "scripts": {
        "kep-php-cs-fixer": "php-cs-fixer --config=./.php-cs-fixer --allow-risky=yes"
    }
}
